{
    "name": "superv/platform",
    "description": "SuperV Platform Package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "admin",
        "laravel-admin",
        "php",
        "dashboard",
        "admin-panel",
        "modular",
        "laravel-extension"
    ],
    "autoload": {
        "psr-4": {
            "SuperV\\Platform\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Platform\\": "tests/Platform/"
        }
    },
    "require": {
        "php": "^7.4",
        "superv/composer-plugin": "^0.2",
        "laravel/framework": "^7.0",
        "fruitcake/laravel-cors": "^1.0",
        "tymon/jwt-auth": "^1.0",
        "nicmart/string-template": "~0.1",
        "wikimedia/composer-merge-plugin": "^1.4"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "SuperV\\Platform\\PlatformServiceProvider"
            ]
        }
    }
    
}
