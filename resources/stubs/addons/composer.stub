{
    "name": "{addon.vendor}/{addon.name}-{addon.type}",
    "type": "superv-{addon.type}",
    "autoload": {
        "psr-4": {
            "{psr4_prefix}\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require": {
        "superv/platform": "0.*|dev-develop"
    },
    "minimum-stability": "dev"
}

